<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/TempleteNguoiDung/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/fonts/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/fonts/themify/themify-icons.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/fonts/Linearicons-Free-v1.0.0/icon-font.min.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/fonts/elegant-font/html-css/style.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/vendor/animate/animate.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/vendor/animsition/css/animsition.min.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/vendor/select2/select2.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/vendor/slick/slick.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/vendor/noui/nouislider.min.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/css/util.css" rel="stylesheet" />
    <link href="~/Content/TempleteNguoiDung/css/main.css" rel="stylesheet" />


</head>

<body class="animsition">

    <!-- Header -->
    <header class="header1">
        <!-- Header desktop -->
        <div class="container-menu-header">
            <div class="topbar">
                <div class="topbar-social">
                    <a href="#" class="topbar-social-item fa fa-facebook"></a>
                    <a href="#" class="topbar-social-item fa fa-instagram"></a>
                    <a href="#" class="topbar-social-item fa fa-pinterest-p"></a>
                    <a href="#" class="topbar-social-item fa fa-snapchat-ghost"></a>
                    <a href="#" class="topbar-social-item fa fa-youtube-play"></a>
                </div>

                <span class="topbar-child1">
                    Free shipping for standard order over 150.000 VND
                </span>

                <div class="topbar-child2">
                    <span class="topbar-email">
                        nguyensimnts@gmail.com
                    </span>

                  
                </div>
            </div>

            <div class="wrap_header">
                <!-- Logo -->
                <a href="@Url.Action("Index","Home")" class="logo">
                    <img src="~/images/logo/logof.png" />

                   
                </a>

                <!-- Menu -->
                <div class="wrap_menu">
                    <nav class="menu">
                        <ul class="main_menu">
                            <li class="sale-noti">
                                <a href="@Url.Action("Index","Home")">Home</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index","Home")">Shop</a>
                                <ul class="sub_menu">
                                    @Html.Action("LoaiHangMenuPartial", "LoaiHang")
                                </ul>
                            </li>                           

                            <li >
                                <a href="@Url.Action("HangSale","Hang")">Sale</a>
                            </li>

                            <li>
                                <a href="cart.html">Features</a>
                            </li>

                            <li>
                                <a href="blog.html">Blog</a>
                            </li>

                            <li>
                                <a href="about.html">About</a>
                            </li>

                            <li>
                                <a href="contact.html">Contact</a>
                            </li>
                        </ul>
                    </nav>
                </div>

                <!-- Header Icon -->
                <div class="header-icons">
                    <a href="#" class="header-wrapicon1 dis-block">
                        @if (Session["UserName"] != null)
                        {<text>@Session["UserName"].ToString()</text> }
                     <img src="~/Content/TempleteNguoiDung/images/icons/icon-header-01.png" class="header-icon1" alt="ICON" />
                          
                    </a>

                    <span class="linedivide1"></span>
                    @Html.Action("GioHangPartial", "GioHang")
                </div>
            </div>
        </div>

        <!-- Header Mobile -->
        <div class="wrap_header_mobile">
            <!-- Logo moblie -->
            <a href="index.html" class="logo-mobile">
                <img src="images/icons/logo.png" alt="IMG-LOGO">
            </a>

            <!-- Button show menu -->
            <div class="btn-show-menu">
                <!-- Header Icon mobile -->
                <div class="header-icons-mobile">
                    <a href="#" class="header-wrapicon1 dis-block">
                        @if (Session["UserName"] != null)
                        {<text>@Session["UserName"].ToString()</text> }
                        
                        <img src="~/Content/TempleteNguoiDung/images/icons/icon-header-01.png" class="header-icon1" alt="ICON" />
                            
                    </a>

                    <span class="linedivide1"></span>
                    @Html.Action("GioHangPartial", "GioHang")
                </div>
                </div>
        </div>

        <!-- Menu Mobile -->
        <div class="wrap-side-menu">
            <nav class="side-menu">
                <ul class="main-menu">
                    <li class="item-topbar-mobile p-l-20 p-t-8 p-b-8">
                        <span class="topbar-child1">
                            Free shipping for standard order over 150.000 VND
                        </span>
                    </li>

                    <li class="item-topbar-mobile p-l-20 p-t-8 p-b-8">
                        <div class="topbar-child2-mobile">
                            <span class="topbar-email">
                                nguyensimnts@gmail.com
                            </span>

                           
                        </div>
                    </li>

                    <li class="item-topbar-mobile p-l-10">
                        <div class="topbar-social-mobile">
                            <a href="#" class="topbar-social-item fa fa-facebook"></a>
                            <a href="#" class="topbar-social-item fa fa-instagram"></a>
                            <a href="#" class="topbar-social-item fa fa-pinterest-p"></a>
                            <a href="#" class="topbar-social-item fa fa-snapchat-ghost"></a>
                            <a href="#" class="topbar-social-item fa fa-youtube-play"></a>
                        </div>
                    </li>


                    <li class="item-menu-mobile">
                        <a href="@Url.Action("Index","Home")">Home</a>
                        
                    </li>

                    <li class="item-menu-mobile">
                        <a href="@Url.Action("Index","Home")">Shop</a>
                        <ul class="sub-menu">
                            @Html.Action("LoaiHangMenuPartial", "LoaiHang")
                        </ul>
                        <i class="arrow-main-menu fa fa-angle-right" aria-hidden="true"></i>
                    </li>

                    <li class="item-menu-mobile">
                        <a href="@Url.Action("HangSale","Hang")">Sale</a>
                    </li>

                    <li class="item-menu-mobile">
                        <a href="cart.html">Features</a>
                    </li>

                    <li class="item-menu-mobile">
                        <a href="blog.html">Blog</a>
                    </li>

                    <li class="item-menu-mobile">
                        <a href="about.html">About</a>
                    </li>

                    <li class="item-menu-mobile">
                        <a href="contact.html">Contact</a>
                    </li>
                </ul>
            </nav>
        </div>
    </header>



    <!-- Content page -->
    <section class="bgwhite p-t-55 p-b-65">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-md-4 col-lg-3 p-b-50">
                    <div class="leftbar p-r-20 p-r-0-sm">
                        <!--  -->
                        <div>
                            @Html.Action("LoaiHangPartial","LoaiHang")
                        </div>
                        <div>
                            @Html.Action("ThuongHieuPartial","ThuongHieu")
                        </div>

                        <!--  -->
                        <h4 class="m-text14 p-b-32">
                            Tìm kiếm
                        </h4>                       
                        <div class="search-product pos-relative bo4 of-hidden">
                            @using (Html.BeginForm("KetQuaTimKiem","TimKiem",FormMethod.Post))
                            {
                                @Html.TextBox("txtTimKiem","",htmlAttributes: new { @class = "s-text7 size6 p-l-23 p-r-50", @placeholder = "Search Products..." })
                                <button class="flex-c-m size5 ab-r-m color2 color0-hov trans-0-4">
                                    <i class="fs-12 fa fa-search" aria-hidden="true"></i>
                                </button>
                            }
                            
                        </div>
                        <!--  -->
                        <h4 class="m-text14 p-b-32">
                            Price:
                        </h4>
                        <div class="search-product pos-relative bo4 of-hidden">
                            @using (Html.BeginForm("KetQuaPrice", "TimKiem", FormMethod.Post))
                            {
                                @Html.TextBox("txtMin","", htmlAttributes: new {@class="sim11", @placeholder = "min" })
                                <span>||</span>
                                
                                @Html.TextBox("txtMax", "", htmlAttributes: new { @class = "sim11", @placeholder = "max" })
                                
                                <button class="flex-c-m size5 ab-r-m color2 color0-hov trans-0-4">
                                    <i class="fs-12 fa fa-search" aria-hidden="true"></i>
                                </button>
                            }

                        </div>
                        <!--  -->
                        <div id="SanPhamMoi">
                            @Html.Action("HangMoiPartial","Hang")
                        </div>
                    </div>
                </div>

                <div class="col-sm-6 col-md-8 col-lg-9 p-b-50">
                    <div class="row">
                        @RenderBody()

                        </div>
                    </div>
            </div>
        </div>
    </section>


    <!-- Footer -->
    <footer class="bg6 p-t-45 p-b-43 p-l-45 p-r-45">
        <div class="flex-w p-b-90">
            <div class="w-size6 p-t-30 p-l-15 p-r-15 respon3">
                <h4 class="s-text12 p-b-30">
                    GET IN TOUCH
                </h4>

                <div>
                    <p class="s-text7 w-size27">
                        Any questions? Let us know in store at 8th floor, 379 Hudson St, New York, NY 10018 or call us on (+1) 96 716 6879
                    </p>

                    <div class="flex-m p-t-30">
                        <a href="#" class="fs-18 color1 p-r-20 fa fa-facebook"></a>
                        <a href="#" class="fs-18 color1 p-r-20 fa fa-instagram"></a>
                        <a href="#" class="fs-18 color1 p-r-20 fa fa-pinterest-p"></a>
                        <a href="#" class="fs-18 color1 p-r-20 fa fa-snapchat-ghost"></a>
                        <a href="#" class="fs-18 color1 p-r-20 fa fa-youtube-play"></a>
                    </div>
                </div>
            </div>
           
            <div class="w-size7 p-t-30 p-l-15 p-r-15 respon4">
                <h4 class="s-text12 p-b-30">
                   
                    Help
                </h4>

                <ul>
                    <li class="p-b-9">
                        <a href="#" class="s-text7">
                            Track Order
                        </a>
                    </li>

                    <li class="p-b-9">
                        <a href="#" class="s-text7">
                            Returns
                        </a>
                    </li>

                    <li class="p-b-9">
                        <a href="#" class="s-text7">
                            Shipping
                        </a>
                    </li>

                    <li class="p-b-9">
                        <a href="#" class="s-text7">
                            FAQs
                        </a>
                    </li>
                </ul>
            </div>

            <div class="w-size8 p-t-30 p-l-15 p-r-15 respon3">
                <h4 class="s-text12 p-b-30">
                    Newsletter
                </h4>

                <form>
                    <div class="effect1 w-size9">
                        <input class="s-text7 bg6 w-full p-b-5" type="text" name="email" placeholder="email@example.com">
                        <span class="effect1-line"></span>
                    </div>

                    <div class="w-size2 p-t-20">
                        <!-- Button -->
                        <button class="flex-c-m size2 bg4 bo-rad-23 hov1 m-text3 trans-0-4">
                            Subscribe
                        </button>
                    </div>

                </form>
            </div>

            <div class="w-size7 p-t-30 p-l-15 p-r-15 respon4">
                <h4 class="s-text12 p-b-30">
                
                    Thống kê
                </h4>
                @{
                    HttpApplicationState thongke = HttpContext.Current.ApplicationInstance.Application;
                }
                <ul>
                    <li class="p-b-9">
                        <a href="#" class="s-text7">
                            UserOnline:@thongke["Online"]
                        </a>
                    </li>

                    <li class="p-b-9">
                        <a href="#" class="s-text7">
                           Tổng số lượt đã truy cập: @thongke["SLTruyCap"]
                        </a>
                    </li>

                  
                </ul>
            </div>
        </div>

        <div class="t-center p-l-15 p-r-15">         
            <div class="t-center s-text8 p-t-20">
                Copyright © 2018 All rights reserved. | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
            </div>
        </div>
    </footer>



    <!-- Back to top -->
    <div class="btn-back-to-top bg0-hov" id="myBtn">
        <span class="symbol-btn-back-to-top">
            <i class="fa fa-angle-double-up" aria-hidden="true"></i>
        </span>
    </div>

    <!-- Container Selection -->
    <div id="dropDownSelect1"></div>
    <div id="dropDownSelect2"></div>


    <script src="~/Content/TempleteNguoiDung/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/bootstrap/js/popper.js"></script>
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/select2/select2.min.js"></script>
    <script type="text/javascript">
		$(".selection-1").select2({
			minimumResultsForSearch: 20,
			dropdownParent: $('#dropDownSelect1')
		});

		$(".selection-2").select2({
			minimumResultsForSearch: 20,
			dropdownParent: $('#dropDownSelect2')
		});
    </script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/daterangepicker/moment.min.js"></script>
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/slick/slick.min.js"></script>
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/js/slick-custom.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/sweetalert/sweetalert.min.js"></script>
    <script type="text/javascript">
		$('.block2-btn-addcart').each(function(){
			var nameProduct = $(this).parent().parent().parent().find('.block2-name').html();
			$(this).on('click', function(){
				swal(nameProduct, "is added to cart !", "success");
			});
		});

		$('.block2-btn-addwishlist').each(function(){
			var nameProduct = $(this).parent().parent().parent().find('.block2-name').html();
			$(this).on('click', function(){
				swal(nameProduct, "is added to wishlist !", "success");
			});
		});
    </script>

    <!--===============================================================================================-->
    <script type="text/javascript" src="~/Content/TempleteNguoiDung/vendor/noui/nouislider.min.js"></script>
    <script type="text/javascript">
		/*[ No ui ]
	    ===========================================================*/
	    var filterBar = document.getElementById('filter-bar');

	    noUiSlider.create(filterBar, {
	        start: [ 50, 200 ],
	        connect: true,
	        range: {
	            'min': 50,
	            'max': 200
	        }
	    });

	    var skipValues = [
	    document.getElementById('value-lower'),
	    document.getElementById('value-upper')
	    ];

	    filterBar.noUiSlider.on('update', function( values, handle ) {
	        skipValues[handle].innerHTML = Math.round(values[handle]) ;
	    });
    </script>
    <!--===============================================================================================-->
    <script src="~/Content/TempleteNguoiDung/js/main.js"></script>

</body>

</html>
<style>
    .logo img {
    max-height: 60px;
    margin-left: 70px;
}
    .sim11{
        width:80px;
        border-right:solid 1px;
    }
</style>